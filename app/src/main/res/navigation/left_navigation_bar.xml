<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/left_navigation_bar"
    app:startDestination="@id/mainViewFragment">

    <fragment
        android:id="@+id/mainViewFragment"
        android:name="com.example.bilibili.navigation.headpage.MainViewFragment"
        android:label="fragment_main_view"
        tools:layout="@layout/fragment_main_view" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.bilibili.navigation.history.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/laterFragment"
        android:name="com.example.bilibili.navigation.later.LaterFragment"
        android:label="fragment_letter"
        tools:layout="@layout/fragment_letter" />
    <fragment
        android:id="@+id/downloadFragment"
        android:name="com.example.bilibili.navigation.download.DownloadFragment"
        android:label="fragment_download"
        tools:layout="@layout/fragment_download" />
    <fragment
        android:id="@+id/collectionFragment"
        android:name="com.example.bilibili.navigation.collection.CollectionFragment"
        android:label="fragment_collection"
        tools:layout="@layout/fragment_collection" />
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.example.bilibili.navigation.headpage.mainviews.headpage.view.fragments.recommand.fragments.MessageFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_message" />
</navigation>